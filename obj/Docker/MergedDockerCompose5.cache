services:
  redis:
    container_name: cache
    expose:
    - '6379'
    image: redis:alpine
  redis.consumer:
    build:
      context: C:\makaleler\github\redispubsup
      dockerfile: Redis.Consumer/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: redisconsumer
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\inan\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\inan\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
  redis.producer:
    build:
      context: C:\makaleler\github\redispubsup
      dockerfile: Redis.Producer/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: redisproducer
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\inan\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\inan\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'