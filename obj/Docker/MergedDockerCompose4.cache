services:
  redis.consumer:
    build:
      context: C:\makaleler\github\redispubsup
      dockerfile: Redis.Consumer/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Redis.Consumer
      target: base
    container_name: Redis.Consumer
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: redisconsumer:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/Redis.Consumer.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\makaleler\github\redispubsup\Redis.Consumer:/app:rw
    - C:\Users\inan\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\inan\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\inan\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\inan\.nuget\packages:/root/.nuget/packages:ro
    - C:\makaleler\github\redispubsup:/src:rw
  redis.producer:
    build:
      context: C:\makaleler\github\redispubsup
      dockerfile: Redis.Producer/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Redis.Producer
      target: base
    container_name: Redis.Producer
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: redisproducer:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/Redis.Producer.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\makaleler\github\redispubsup\Redis.Producer:/app:rw
    - C:\Users\inan\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\inan\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\inan\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\inan\.nuget\packages:/root/.nuget/packages:ro
    - C:\makaleler\github\redispubsup:/src:rw
version: '3.4'